{
  "openapi": "3.0.3",
  "info": {
    "version": "1.0.0",
    "title": "Divblox api",
    "description": "Divblox api"
  },
  "servers": [
    {
      "url": "/api/",
      "description": "The server api root"
    }
  ],
  "tags": [
    {
      "name": "EndpointOne",
      "description": "An example of grouping endpoint one operations"
    },
    {
      "name": "EndpointTwo",
      "description": "An example of grouping endpoint two operations"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/endpointOne/operationOne": {
      "post": {
        "tags": [
          "EndpointOne"
        ],
        "summary": "Executes the operationOne operation in the endpoint one grouping",
        "parameters": [
          {
            "$ref": "#/components/jwt"
          }
        ],
        "requestBody": {
          "description": "Anything that describes the request body",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ExampleEntity"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content" : {
              "application/json" : {
                "schema": {
                  "$ref": "#/components/message",
                  "$ref": "#/components/schemas/ExampleEntity"
                }
              }
            }
          },
          "400": {
            "description": "Failed. Bad post data.",
            "content" : {
              "application/json" : {
                "result": {
                  "$ref": "#/components/result"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "jwt": {
      "in": "header",
      "name": "Authorization",
      "required": true,
      "description": "JSON Web Token",
      "schema": {
        "$ref": "#/components/auth"
      }
    },
    "auth": {
      "properties": {
        "Bearer": {
          "type": "string"
        }
      }
    },
    "message": {
      "type": "string"
    },
    "schemas": {
      "ExampleEntity": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "attributeOne": {
            "type": "string"
          },
          "attributeTwo": {
            "type": "string"
          }
        }
      }
    }
  }
}